<h1> Sign In </h1>
<br>
<%= form_tag({controller: 'sessions', action: 'create', method: 'post'}) do %>
  <%= label_tag(:'user[email]', "email") %>
  <%= text_field_tag(:'user[email]', params[:login]) %><br>
  <%= label_tag(:'user[password]', "Password") %>
  <%= password_field_tag(:'user[password]', params[:password]) %><br><br>
  <%= submit_tag("Sign In") %>
<% end %>
<br>
